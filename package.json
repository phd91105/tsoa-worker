{
  "name": "tsoa-worker",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "deploy": "run tsoa && wrangler deploy --minify",
    "dev": "wrangler dev",
    "dev:bun": "bun --hot src/index.ts",
    "format": "prettier --write src/**/*.ts",
    "postinstall": "run tsoa && husky install",
    "lint": "eslint --fix src/**/*.ts --quiet",
    "prisma:generate": "prisma generate --no-engine",
    "prisma:sync": "prisma db push",
    "tsoa": "tsoa spec-and-routes"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "@prisma/client": "^5.8.1",
    "@prisma/extension-accelerate": "^0.6.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.10",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "hono": "^3.12.6",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "lodash": "^4.17.21",
    "prettier": "^3.2.4",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prisma": "^5.8.1",
    "ts-node": "^10.9.2",
    "tsoa": "^6.0.1",
    "tsyringe": "^4.8.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.23.0"
  },
  "packageManager": "yarn@4.0.2"
}
