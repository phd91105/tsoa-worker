{
  "name": "tsoa-worker",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "workspaces": [
    "batch"
  ],
  "scripts": {
    "deploy": "run tsoa && wrangler deploy --minify",
    "dev": "run tsoa && wrangler dev",
    "format": "prettier --write {src,batch,templates}/**/*.{ts,tsx,mjs}",
    "lint": "eslint --fix {src,batch,templates}/**/*.{ts,tsx,mjs} --quiet",
    "prepare": "run tsoa && husky install",
    "prisma:generate": "prisma generate --no-engine",
    "prisma:sync": "dotenv -e .dev.vars -- prisma db push",
    "tsoa": "tsoa spec-and-routes"
  },
  "lint-staged": {
    "{src,batch,templates}/**/*.{ts,tsx,mjs}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240129.0",
    "@prisma/client": "^5.9.0",
    "@prisma/extension-accelerate": "^0.6.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.15",
    "@types/papaparse": "^5.3.14",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.10",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "hono": "^3.12.9",
    "husky": "^9.0.7",
    "itty-fetcher": "^0.9.4",
    "lint-staged": "^15.2.1",
    "lodash": "^4.17.21",
    "papaparse": "^5.4.1",
    "prettier": "^3.2.4",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prisma": "^5.9.0",
    "ts-node": "^10.9.2",
    "tsoa": "^6.0.1",
    "tsyringe": "^4.8.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.26.0"
  },
  "packageManager": "yarn@4.1.0"
}
